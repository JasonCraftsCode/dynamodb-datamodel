{
  "typescript.tsdk": "node_modules/typescript/lib",
  "jestrunner.runOptions": ["--no-cache"],
  "jest.enableInlineErrorMessages": true,
  "eslint.workingDirectories": ["src/**/*.ts", "examples/**/*.ts", "__test__/**/*.ts"],
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.options": {},
  "cSpell.words": ["Birdland", "keyof", "prepend", "tsdoc", "unscoped"],
  "files.associations": {
    "api-extractor.json": "jsonc"
  },
  "auto-close-tag.activationOnLanguage": ["html", "xml", "javascript", "javascriptreact", "typescriptreact"]
}
